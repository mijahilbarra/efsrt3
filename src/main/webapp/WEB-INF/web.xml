<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>UsuarioServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.UsuarioServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UsuarioServlet</servlet-name>
    <url-pattern>/usuario</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ProyectoServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.ProyectoServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProyectoServlet</servlet-name>
    <url-pattern>/proyecto</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TutoriaServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.TutoriaServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TutoriaServlet</servlet-name>
    <url-pattern>/tutoria</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InscritosServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.InscritosServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InscritosServlet</servlet-name>
    <url-pattern>/inscritos</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ExplorarTutoriasServlet</servlet-name>
    <servlet-class>com.cibertec.servlet.ExplorarTutoriasServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ExplorarTutoriasServlet</servlet-name>
    <url-pattern>/explorar-tutorias</url-pattern>
  </servlet-mapping>
</web-app>
